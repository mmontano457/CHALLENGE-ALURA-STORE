        "def promedio_calificacion(df):\n",
        "  return round(float(df['Calificación'].mean()),3)\n",
        "promedio_x_tienda = {'Tienda 1': promedio_calificacion(tienda1), \"Tienda 2\": promedio_calificacion(tienda2), \"Tienda 3\": promedio_calificacion(tienda3), \"Tienda 4\": promedio_calificacion(tienda4)}\n",
        "print(promedio_x_tienda)\n",
        "etiquetas = list(promedio_x_tienda.keys())\n",
        "valores = list(promedio_x_tienda.values())\n",
        "fig, ax = plt.subplots(figsize=(6, 8))\n",
        "wedges, texts = ax.pie(valores, startangle=50)\n",
        "ax.axis('equal')\n",
        "\n",
        "# Añadir etiquetas personalizadas (nombre + valor)\n",
        "for i, text in enumerate(texts):\n",
        "    text.set_text(f'{etiquetas[i]}: {valores[i]}')\n",
        "\n",
        "ax.set_title('Calificacion por tienda')\n",
        "plt.axis('equal')\n",
        "plt.show()\n"
      ]
