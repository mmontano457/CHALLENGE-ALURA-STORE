        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "productos_1 = tienda1.groupby('Producto')['Precio'].sum().sort_values(ascending=False)\n",
        "productos_2 = tienda2.groupby('Producto')['Precio'].sum().sort_values(ascending=False)\n",
        "productos_3 = tienda3.groupby('Producto')['Precio'].sum().sort_values(ascending=False)\n",
        "productos_4 = tienda4.groupby('Producto')['Precio'].sum().sort_values(ascending=False)\n",
        "\n",
        "ventas_comparadas = pd.concat([productos_1, productos_2, productos_3, productos_4], axis=1, keys=['Tienda 1', 'Tienda 2', 'Tienda 3', 'Tienda 4']).fillna(0)\n",
        "\n",
        "\n",
        "# Obtener los productos más vendidos combinados (top 10)\n",
        "ventas_comparadas['Total'] = ventas_comparadas['Tienda 1'] + ventas_comparadas['Tienda 2'] +ventas_comparadas['Tienda 3']+ ventas_comparadas['Tienda 4']\n",
        "top_productos = ventas_comparadas.sort_values('Total', ascending=False).head(10)\n",
        "menores_productos = ventas_comparadas.sort_values('Total', ascending=True).head(10)\n",
        "# Eliminar la columna Total para el gráfico\n",
        "top_productos = top_productos.drop(columns='Total')\n",
        "menores_productos = menores_productos.drop(columns='Total')\n",
        "\n",
        "# Graficar top\n",
        "productos = top_productos.index.tolist()\n",
        "x = np.arange(len(productos))  # posiciones de las barras\n",
        "\n",
        "bar_width = 0.2  # ancho de cada barra\n",
        "\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "plt.bar(x - 1.5*bar_width, top_productos['Tienda 1'], width=bar_width, label='Tienda 1', color='darkred')\n",
        "plt.bar(x - 0.5*bar_width, top_productos['Tienda 2'], width=bar_width, label='Tienda 2', color='darkorange')\n",
        "plt.bar(x + 0.5*bar_width, top_productos['Tienda 3'], width=bar_width, label='Tienda 3', color='darkgreen')\n",
        "plt.bar(x + 1.5*bar_width, top_productos['Tienda 4'], width=bar_width, label='Tienda 4', color='blueviolet')\n",
        "\n",
        "plt.xlabel('Producto')\n",
        "plt.ylabel('Precio Total')\n",
        "plt.title('Top 10 Productos Más Vendidos - Comparativa por Tienda')\n",
