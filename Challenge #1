        "# 2. Ventas por categoría"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 35,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 607
        },
        "id": "A34YupIuYhfF",
        "outputId": "364b1af1-3e49-45a4-c261-4ab124cc60a7"
      },
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 1200x600 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "def ventas_categoria(df):\n",
        "  return df.groupby('Categoría del Producto')['Precio'].sum()\n",
        "tiendas = {\n",
        "    'Tienda 1': tienda1,\n",
        "    'Tienda 2': tienda2,\n",
        "    'Tienda 3': tienda3,\n",
        "    'Tienda 4': tienda4\n",
        "}\n",
        "\n",
        "# Obtener todas las categorías posibles\n",
        "categorias = set()\n",
        "for tienda in tiendas.values():\n",
        "    categorias.update(ventas_categoria(tienda).keys())\n",
        "categorias = sorted(categorias)\n",
        "\n",
        "# Crear matriz de ventas: filas = categorías, columnas = tiendas\n",
        "datos = []\n",
        "for tienda in tiendas.values():\n",
        "    ventas = ventas_categoria(tienda)\n",
        "    fila = [ventas.get(cat, 0) for cat in categorias]\n",
        "    datos.append(fila)\n",
        "\n",
        "# Transponer para graficar por categoría\n",
        "datos = np.array(datos).T\n",
        "x = np.arange(len(categorias))\n",
        "ancho = 0.25\n",
        "\n",
        "plt.figure(figsize=(12, 6))\n",
        "for i, (nombre_tienda, _) in enumerate(tiendas.items()):\n",
        "    plt.bar(x + i*ancho, datos[:, i], width=ancho, label=nombre_tienda)\n",
        "\n",
        "plt.xticks(x + ancho, categorias, rotation=45)\n",
        "plt.xlabel('Categoría del Producto')\n",
        "plt.ylabel('Ventas')\n",
        "plt.title('Comparación de Ventas por Categoría y Tienda')\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()"
      ]
    },
